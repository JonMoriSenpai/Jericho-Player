{
  "name": "jericho-player",
  "version": "1.0.0",
  "description": "Jericho Player Framework for discord.js and @discordjs/voice Supported Bots for Music Player and Radio Players",
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "types": "./typings/index.d.ts",
  "files": [
    "src",
    "typings"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.mjs"
    }
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main",
    "commit:push": "git add . && npm run commit && git push"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "nyc": "^15.1.0",
    "semantic-release": "^18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 16.6.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@discordjs/voice": "^0.6.0",
    "discord.js": "^13.2.0",
    "event-emitter": "^0.3.5",
    "is-url": "^1.2.4",
    "playdl-music-extractor": "^2.0.6",
    "prism-media": "^1.3.2"
  },
  "directories": {
    "example": "example"
  },
  "author": "Sid is Live YT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jericho-Player/Jericho-Player.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jericho-Player/Jericho-Player/issues"
  },
  "homepage": "https://github.com/Jericho-Player/Jericho-Player#readme"
}
