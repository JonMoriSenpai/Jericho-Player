{
  "name": "jericho-player",
  "version": "1.3.0",
  "description": "LightWeight Framework for eris.js 1.7.2#dev Music Bots and Radio Bots with fast moderation with commands and no memory leak mode",
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "types": "./typings/index.d.ts",
  "files": [
    "src",
    "typings"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.mjs"
    }
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "lint:commit": "npm run lint:fix && npm run commit:push",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main",
    "commit:push": "git add . && npm run commit && git push",
    "docs": "typedoc --cname jericho-player.js.org --name Jericho-Player --includeVersion --readme --hideGenerator --out docs typings/index.d.ts",
    "test": "node test/noden.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 16.6.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "eris": "github:abalabahaha/eris#dev",
    "events": "^3.3.0",
    "ffmpeg-static": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "isomorphic-unfetch": "^3.1.0",
    "linkifyjs": "^4.0.2",
    "node-youtube-music": "^0.8.3",
    "play-dl": "^1.9.6",
    "scdl-core": "^1.1.0",
    "spotify-url-info": "^3.1.9",
    "spotify-web-api-node": "^5.0.2"
  },
  "directories": {
    "example": "example"
  },
  "author": "Sid is Live YT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SidisLiveYT/Jericho-Player.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SidisLiveYT/Jericho-Player/issues"
  },
  "homepage": "https://jericho-player.js.org",
  "keywords": [
    "jericho-player",
    "music-player",
    "youtube",
    "discord-music-player",
    "spotify",
    "soundcloud",
    "facebook",
    "player-framework",
    "player",
    "youtube-dl",
    "play-dl",
    "music-framework",
    "discord bots",
    "distube",
    "youtube-player",
    "ytdl",
    "ytdl-player",
    "youtube-framework",
    "discord-music-player",
    "discord-player"
  ],
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/node": "^18.11.9",
    "@types/spotify-web-api-node": "^5.0.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  }
}
