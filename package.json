{
  "name": "jericho-player",
  "version": "1.0.0",
  "description": "LightWeight Framework for discord.js v13 Music Bots and Radio Bots with fast moderation with commands and no memory leak",
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "types": "./typings/index.d.ts",
  "files": [
    "src",
    "typings"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.mjs"
    }
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main",
    "commit:push": "git add . && npm run commit && git push",
    "docs": "jsdoc node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "docs:advance": "docgen --jsdoc jsdoc.json --source src --custom docs/index.yml --output docs/docs.json"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@discordjs/docgen": "^0.10.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "jsdoc": "^3.6.7",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-skyceil": "Androz2091/jsdoc-skyceil",
    "nyc": "^15.1.0",
    "semantic-release": "^18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 16.6.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@discordjs/voice": "^0.7.2",
    "discord.js": "^13.3.1",
    "is-url": "^1.2.4",
    "playdl-music-extractor": "^2.3.5",
    "prism-media": "^1.3.2",
    "youtube-suggest-gen": "^1.0.1"
  },
  "directories": {
    "example": "example"
  },
  "author": "Sid is Live YT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jericho-Player/Jericho-Player.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jericho-Player/Jericho-Player/issues"
  },
  "homepage": "https://jericho-player.js.org/"
}
