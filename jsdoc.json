{
  "source": {
    "include": ["package.json", "README.md", "src", "src/types/interfaces.js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|tests/)"
  },
  "plugins": ["plugins/markdown", "node_modules/jsdoc-babel"],
  "babel": {
    "extensions": ["js"],
    "babelrc": false,
    "presets": [["@babel/preset-env", { "targets": { "node": true } }]]
  }
}
