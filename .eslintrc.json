{
  "root": true,
  "parser": "@babel/eslint-parser",
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "max-len": "off",
    "no-use-before-define": "off",
    "import/named": "off",
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "no-unused-vars": "off",
    "no-shadow": "off",
    "no-return-await": "off",
    "no-void": "off",
    "no-plusplus": "off",
    "no-loop-func": "off",
    "no-param-reassign": "off",
    "radix": "off",
    "global-require": "off",
    "no-useless-escape": "off"
  }
}
